post_install() {
  echo "Execute /opt/ecodms/ecodmsclient/ecodmsconmgr.sh to connect to the server"
  echo "Update the symlink /opt/ecodms/ecodmsclient/language.qm to change the UI language"
}

post_upgrade() {
  post_install
}

post_remove() {
  rm -rf /opt/ecodms/ecodmsclient
}

