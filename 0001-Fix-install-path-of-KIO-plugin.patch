+    set(META_PLUGIN_CATEGORY kf${KF_MAJOR_VERSION}/kfileitemaction)
 else ()
     list(APPEND KIO_MODULE_TARGETS "${KF_PACKAGE_PREFIX}::KIONTLM")
 endif ()
