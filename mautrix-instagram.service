[Unit]
Description=Matrix-Instagram DM puppeting bridge

[Service]
ExecStart=python -m mautrix_instagram -b /usr/share/mautrix-instagram/example-config.yaml -c /etc/mautrix-instagram/config.yaml -r /etc/mautrix-instagram/registration.yaml
Restart=on-failure
User=mautrix-instagram
WorkingDirectory=~


[Install]
WantedBy=multi-user.target
