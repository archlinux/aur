#%Module1.0

proc ModulesHelp { } {
        global version

        puts stderr "\tSet up environment for AmberTools19 package"
}

module-whatis "AmberTools19 package"

set             AMBER_PREFIX    /opt/ambertools
set             _gccver         8.3.0

setenv          AMBER_PREFIX    ${AMBER_PREFIX}
setenv          AMBERHOME       ${AMBER_PREFIX}

prepend-path    PATH            ${AMBER_PREFIX}/bin
prepend-path    LD_LIBRARY_PATH ${AMBER_PREFIX}/lib
prepend-path    PYTHONPATH      ${AMBER_PREFIX}/lib/python2.7/site-packages
prepend-path    LD_LIBRARY_PATH /usr/lib/gcc/x86_64-pc-linux-gnu/${_gccver}
