+									}
+									/* Apply the change as appropriate */
+									a24xx_spi_setreg(wc_dev, x, LINE_STATE, fxs->lasttxhook);
+							}
+					} else {
+							fxs->ohttimer = 0;
+							/* Switch to active */
+							fxs->idletxhookstate = POLARITY_XOR(x) ? SLIC_LF_ACTIVE_REV : SLIC_LF_ACTIVE_FWD;
+							printk("Channel %d OnHookTransfer abort\n",x);
+					}
