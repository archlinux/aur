+												} 
+												else if (wc_dev->modtype[x] == MOD_TYPE_FXO) {
+														a24xx_voicedaa_check_hook(wc, x);
+												}
+												break;
+										}
+								}
+						}
+						break;
+				case 16:
+						for(x=0;x<wc_dev->max_cards;x++){
