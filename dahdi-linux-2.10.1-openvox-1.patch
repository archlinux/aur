+												wc_dev->reg0shadow[x] = a24xx_spi_getreg(wc_dev, x, 68);
+												wc_dev->reg1shadow[x] = a24xx_spi_getreg(wc_dev, x, 64);
+												a24xx_proslic_check_hook(wc, x);
+												if (!(wc_dev->intcount & (0xf0>>4))) {
