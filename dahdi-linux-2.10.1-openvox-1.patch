+												break;
+										case 4:
+												/* Read first shadow reg */
+												if (wc_dev->modtype[x] == MOD_TYPE_FXS) {
+														wc_dev->reg0shadow[x] = a24xx_spi_getreg(wc_dev, x, 68);
