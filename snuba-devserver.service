[Unit]
Description=Snuba development server
After=network.target

[Service]
User=sentry
Restart=always
Environment=PYTHONPATH=/opt/snuba/usr/lib/python3.10/site-packages/ \
    PATH=/opt/snuba/usr/bin:/usr/bin:/usr/local/bin
ExecStart=/opt/snuba/usr/bin/snuba devserver

[Install]
WantedBy=multi-user.target
