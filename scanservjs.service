[Unit]
Description=scanservjs
After=network.target

[Service]
ExecStart=/usr/share/webapps/scanservjs/server/server.js
Restart=always
User=scanservjs

Environment=PATH=/usr/local/bin:/usr/bin:/bin
Environment=NODE_ENV=production
WorkingDirectory=/usr/share/webapps/scanservjs

[Install]
WantedBy=multi-user.target
