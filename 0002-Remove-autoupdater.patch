-	} else if (isdigit(character[0])) {
-		return kNumberType;
-	} else if (isspace(character[0])) {
-		return kSeparatorType;
-	} else if (ispunct(character[0])) {
-		return kSeparatorType;
-	} else {
-		return kStringType;
-	}
