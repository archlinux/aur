-			parts.push_back(aPart);
-			s = character;
-		} else {
-			// Add character to string and continue
-			s.append(character);
-		}
-		oldType = newType;
-	}
-
-	// Add the last part onto the array
-	parts.push_back(s);
-	return parts;
-}
