     {
         struct wined3d_matrix mat;
-
-        for (i = 1; i < MAX_VERTEX_BLENDS; ++i)
+        for (i = 1; i <= MAX_VERTEX_BLEND_IND_UNF; ++i)
         {
             if (prog->vs.modelview_matrix_location[i] == -1)
                 break;
 
-            get_modelview_matrix(context, state, i, &mat);
