<script type="text/javascript">
$( document ).ready(function() {

  var residential = "https://www.voipreview.org/residential_voip";
  // Adwords tracking.
  if (window.trkConv && $('.gad-submission').length) {
      window.trkConv(0111);
  }

  // Home voiptype redirect.
  $('#edit-voiptype-home').click(function() {
    window.location.replace(residential);
  });

  // GAD extra leadform-landing-page class.
  if ($('.leadform-landing-page #leadform').length) {
    $('.leadform-landing-page').addClass('paged');
  }
  else if ($('.leadform-landing-page #block-leadform-leadform-gad-full').length) {
    $('.leadform-landing-page').addClass('full');
  }

  // Add class to the block-leadform-leadform-simple div.
  $('#leadform').addClass('leadform-step-1');
  var step = 1;
  $('#leadform-step-indicator').text(step);

  // Show leadform call number.
  var d = new Date();
  var dayOfWeek = d.getDay();
  var hour = d.getHours();
  var call_text = "<div class='call-text'><span>Want Free buying advice?</span><label class='call-now'>Call us now at</label><span class='tollfree'><a href='tel:8883824669'><img src='/sites/all/modules/custom/leadform/images/voip-quote.png' alt='VoIP Quote'></a></span></div>";
  // open hours Monday - Friday 7am - 9:pm = open
  /*if (dayOfWeek >= 1 && dayOfWeek <= 5 && hour >= 7 && hour <= 19) {
    $('#lead-callout').html(call_text);
  }*/

  // Npa npxx.
  $('#edit-area-code, #edit-npanxx, #edit-npanxx2').keyup(function(e) {
    if ($(this).val().length === $(this).attr('maxlength')) {
      $(this).parent().next().children('input').focus();
    }
  });

  // Disable button on load.
  $('form.multi-form button.multi-next').prop('disabled', true);

  // Step 1.
  $('input[name="Desired_Users"]').change(function() {
    if (typeof $('input[name="Desired_Users"]:checked').val() !== 'undefined') {
      $('form.multi-form button.multi-next').prop('disabled', false);
    }
  });

  // Step 2.
  $('input[name="VoipType"]').change(function() {
    if (typeof $('input[name="VoipType"]:checked').val() !== 'undefined') {
      $('form.multi-form button.multi-next').prop('disabled', false);
    }
  });

  // Step 3.
  $('input[name="TimeFrame"]').change(function() {
    if (typeof $('input[name="TimeFrame"]:checked').val() !== 'undefined') {
      $('form.multi-form button.multi-next').prop('disabled', false);
    }
  });

  // Step 4.
  $('input[name="Type_Of_Data"]').change(function() {
    if (typeof $('input[name="Type_Of_Data"]:checked').val() !== 'undefined') {
      $('form.multi-form button.multi-next').prop('disabled', false);
    }
  });

  // next step
  $('form.multi-form button.multi-next').click(function() {
    $('fieldset.active').removeClass('active').next('fieldset').addClass('active');
    var cur_width = $('.meterbar span').attr('data-width');
    var new_width = parseInt(cur_width) + 20;
    var max_width = parseInt($('.meterbar span').attr('data-max-width'));
    $('.meterbar span').css("width", new_width+'%');
    $('.meterbar span').attr("data-width", new_width);
    if (new_width === 100) {
      $('.meterbar span').addClass('full-meterbar');
    }
    else {
      $('.meterbar span').removeClass('full-meterbar');
    }
    // Canada rule checkbox and privacy policy / contact us link visibility.
    $('#edit-step-5 #edit-country').change(function() {
      var country = $(this).find('option:selected').val();
      if (country === 'CA') {
        $('.privacy-policy-contact').show();
      }
      else {
        $('.privacy-policy-contact').hide();
      }
    });

    // Disabling the next button.
    if (max_width <= new_width) {
      $(this).prop('disabled', true);
      $('.meterbar span').attr("data-max-width", new_width);
    }
    // Increment step class.
    $('#leadform').removeClass('leadform-step-' + step);
    step++;
    $('#leadform').addClass('leadform-step-' + step);
    $('#leadform-step-indicator').text(step);


  });

  // Step back.
  $('form.multi-form button.multi-prev').click(function() {
    var cur_width = $('.meterbar span').attr('data-width');
    var new_width = parseInt(cur_width) - 20;
    $('.meterbar span').css("width", new_width + '%');
    $('.meterbar span').attr("data-width", new_width);
    if (new_width === 100) {
      $('.meterbar span').addClass('full-meterbar');
    }
    else {
      $('.meterbar span').removeClass('full-meterbar');
    }
    $('fieldset.active').removeClass('active')
      .prev('fieldset').addClass('active');
    $('form.multi-form button.multi-next').removeAttr('disabled');
    // Decrement step class.
    $('#leadform').removeClass('leadform-step-' + step);
    step--;
    $('#leadform').addClass('leadform-step-' + step);
    $('#leadform-step-indicator').text(step);
  });


  // Tooltips.
  $('#leadform .icon-info').hover(function () {
    $('.tooltip.redirect').toggle();
  });

  $('#edit-country').change(function() {
    if ($(this).find('option:selected').text() === 'Canada') {
      $('.form-item-terms, .privacy-policy-contact').show();
    }
    else {
      $('.form-item-terms, .privacy-policy-contact').hide();
    }
    //console.log(selected);
  });

  //submit form
  $('#edit-submit').on('click', function(e) {
        $(this).attr("disabled","disabled");
        e.preventDefault();
        $.ajax({
        type : "POST",
        url : 'https://lf.bizx.info/api/partner/voip',
        dataType : 'json',
        data : $('#leadform-simple-form').serialize(),
        success : function(c) {
            if(c.success === '1') {
              $('#leadform-simple-form').remove();
              $('#leadform').append('<p>' + c.msg + '</p>');
            }
            else {
             $('.leadform-msg').html('<span class="text-error" id="error">'+ c.msg+'</span>');
             $('#edit-submit').attr('disabled', false);
            }
        }
     });
  });
  
  $('form.multi-form button.multi-next').click(function(){
		setTimeout(function(){			
			if($("#edit-step-2").hasClass("active")) {	
				$(".steps li").removeClass("currentStep");		
				$(".steps li.steptop1").addClass("activeStep");
				$(".steps li.steptop2").addClass("currentStep");
			}
			else if($("#edit-step-3").hasClass("active")) {
				$(".steps li").removeClass("currentStep");
				$(".steps li.steptop1, .steps li.steptop2").addClass("activeStep");
				$(".steps li.steptop3").addClass("currentStep");
			}
			else if($("#edit-step-4").hasClass("active")) {
				$(".steps li").removeClass("currentStep");
				$(".steps li.steptop1, .steps li.steptop2, .steps li.steptop3").addClass("activeStep");
				$(".steps li.steptop4").addClass("currentStep");
			}
			else if($("#edit-step-5").hasClass("active")) {
				$(".steps li").removeClass("currentStep");
				$(".steps li.steptop1, .steps li.steptop2, .steps li.steptop3, .steps li.steptop4").addClass("activeStep");
				$(".steps li.steptop5").addClass("currentStep");
			}
			else {
				$(".steps li").removeClass("currentStep activeStep");
				$(".steps li.steptop1").addClass("activeStep");
			}
		}, 100);

                // disable TYP direct if it's planned
                if (SF.downloader && SF.downloader.cancelRedirect) {
                    SF.downloader.cancelRedirect();
                }
	});
	
	$('#leadform .buttons .button.left').click(function(){
		setTimeout(function(){			
			if($("#edit-step-1").hasClass("active")) {	
				$(".steps li").removeClass("currentStep activeStep");
				$(".steps li.steptop1").addClass("activeStep");
			}	
			else if($("#edit-step-2").hasClass("active")) {	
				$(".steps li.steptop3").removeClass("currentStep activeStep");
				$(".steps li.steptop2").addClass("currentStep");
			}	
			else if($("#edit-step-3").hasClass("active")) {	
				$(".steps li.steptop4").removeClass("currentStep activeStep");
				$(".steps li.steptop3").addClass("currentStep");
			}	
			else if($("#edit-step-4").hasClass("active")) {	
				$(".steps li.steptop5").removeClass("currentStep activeStep");
				$(".steps li.steptop4").addClass("currentStep");
			}			
		}, 100);
	});	
});
</script>

 

