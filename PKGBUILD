package_grub-theme-chromeos () {
cd "$pkgname"
name=$(echo "${pkgname}" | sed -e "s/\b\(.\)/\u\1/g" | sed -e "s/^"Grub-Theme-"//")
install -d "$pkgdir"/boot/grub/themes/$name
pkgdesc="${name} Theme for Grub2"
cp -a * "$pkgdir"/boot/grub/themes/$name
}

