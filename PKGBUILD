prepare() {
    # fix some file names
    mv -vf Readme.html Bordersprite_Readme.html
    mv -vf 'Boring Ole Read Me.doc' Blood_Omen_Readme.doc
    mv -vf 'Read Me.html' Astron_Boy_Readme.html
}

package() {
    install -Dm644 -t "$pkgdir/usr/share/fonts/TTF/" *.ttf
    install -Dm644 -t "$pkgdir/usr/share/licenses/$pkgname/" *.txt
    install -Dm644 -t "$pkgdir/usr/share/doc/$pkgname/" *.html *.doc *.pdf
}

