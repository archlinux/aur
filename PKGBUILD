pkgname='jaguar-bin'
pkgver=1.9.26
pkgrel=1
pkgdesc="Tool to develop Toit programs for the ESP32"
arch=('x86_64')
url="https://github.com/toitlang/jaguar"
license=('MIT')
depends=('gcc-libs')
provides=('jaguar')
conflicts=('jaguar')
source=("$pkgname-$pkgver.tgz::$url/releases/download/v$pkgver/jag_linux.tgz")
sha256sums=('75b280deb0a25d7a3ac127688cbadf0d43c346594be728e3b6b2f43897b06267')

package() {
	install -D jag -t "$pkgdir/usr/bin/"
}
