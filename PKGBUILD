# Maintainer: atomicfs

pkgname=linux-beep-music2
_gitname=beep-melodies
pkgver=1.0.0
pkgrel=1
pkgdesc="Collection of more beep melodies"
arch=('any')
url="https://github.com/Josef-Friedrich/beep-melodies"
license=('')
depends=('beep')
makedepends=('git')
source=('git+https://github.com/Josef-Friedrich/beep-melodies.git')
md5sums=('SKIP')

package() {
  cd "${_gitname}"
  install -m 755 -D aerodynamic.sh                     "${pkgdir}/usr/bin/beep-song-aerodynamic2"
  install -m 755 -D alarm_orig.sh                      "${pkgdir}/usr/bin/beep-song-alarm-orig"
  install -m 755 -D alle-meine-entlein.sh              "${pkgdir}/usr/bin/beep-song-alle-meine-entlein"
  install -m 755 -D axel-foley_beverly-hills-cop.sh    "${pkgdir}/usr/bin/beep-song-axel-foley-beverly-hills-cop"
  install -m 755 -D canadian-snowtruck-warning_orig.sh "${pkgdir}/usr/bin/beep-song-canadian-snowtruck-warning-orig"
  install -m 755 -D die-gedanken-sind-frei.sh          "${pkgdir}/usr/bin/beep-song-die-gedanken-sind-frei"
  install -m 755 -D downfall.sh                        "${pkgdir}/usr/bin/beep-song-downfall"
  install -m 755 -D eight-melodies.sh                  "${pkgdir}/usr/bin/beep-song-eight-melodies"
  install -m 755 -D Eine-kleine-Nachmusik_Mozart.sh    "${pkgdir}/usr/bin/beep-song-eine-kleine-nachmusik-mozart"
  install -m 755 -D ffvictory_orig.sh                  "${pkgdir}/usr/bin/beep-song-ffvictory-orig"
  install -m 755 -D Fifth-Symphony_Beethoven.mscx.sh   "${pkgdir}/usr/bin/beep-song-fifth-symphony-beethoven"
  install -m 755 -D final-fantasy.sh                   "${pkgdir}/usr/bin/beep-song-final-fantasy"
  install -m 755 -D fuer-elise.sh                      "${pkgdir}/usr/bin/beep-song-fuer-elise"
  install -m 755 -D full-moon-rock.sh                  "${pkgdir}/usr/bin/beep-song-full-moon-rock"
  install -m 755 -D happy-new-year.sh                  "${pkgdir}/usr/bin/beep-song-happy-new-year"
  install -m 755 -D imperial-march.sh                  "${pkgdir}/usr/bin/beep-song-imperial-march"
  install -m 755 -D jingle-bells.sh                    "${pkgdir}/usr/bin/beep-song-jingle-bells"
  install -m 755 -D mission-impossible_orig.sh         "${pkgdir}/usr/bin/beep-song-mission-impossible-orig"
  install -m 755 -D myth.sh                            "${pkgdir}/usr/bin/beep-song-myth"
  install -m 755 -D pachelbel-canon.sh                 "${pkgdir}/usr/bin/beep-song-pachelbel-canon"
  install -m 755 -D phaser_orig.sh                     "${pkgdir}/usr/bin/beep-song-phaser-orig"
  install -m 755 -D ring_orig.sh                       "${pkgdir}/usr/bin/beep-song-ring-orig"
  install -m 755 -D sandstorm.sh                       "${pkgdir}/usr/bin/beep-song-sandstorm"
  install -m 755 -D scale_orig.sh                      "${pkgdir}/usr/bin/beep-song-scale-orig"
  install -m 755 -D shutdown.sh                        "${pkgdir}/usr/bin/beep-song-shutdown"
  install -m 755 -D startup.sh                         "${pkgdir}/usr/bin/beep-song-startup"
  install -m 755 -D super-mario-overworld.sh           "${pkgdir}/usr/bin/beep-song-super-mario-overworld"
  install -m 755 -D super-mario.sh                     "${pkgdir}/usr/bin/beep-song-super-mario"
  install -m 755 -D super-mario-victory.sh             "${pkgdir}/usr/bin/beep-song-super-mario-victory"
  install -m 755 -D taps_orig.sh                       "${pkgdir}/usr/bin/beep-song-taps-orig"
  install -m 755 -D tetris.sh                          "${pkgdir}/usr/bin/beep-song-tetris"
  install -m 755 -D torrent-completion_orig.sh         "${pkgdir}/usr/bin/beep-song-torrent-completion-orig"
  install -m 755 -D upanddown_orig.sh                  "${pkgdir}/usr/bin/beep-song-upanddown-orig"
  install -m 755 -D vader-jacob.sh                     "${pkgdir}/usr/bin/beep-song-vader-jacob"
  install -m 755 -D zelda_song-of-time.sh              "${pkgdir}/usr/bin/beep-song-zelda-song-of-time"
}

