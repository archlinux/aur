pkgname=libunique
pkgver=1.1.6
pkgrel=8
pkgdesc='Library for writing single instance applications for GTK3'
arch=('i686' 'x86_64')
url='https://wiki.gnome.org/Attic/LibUnique'
license=('GPL')
depends=('gtk3')
#conflicts=('libunique3')
