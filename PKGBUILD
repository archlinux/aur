prepare() {
for t in *.tar ; do
  bn="$(basename "$t" .tar)"
  mkdir -p "$bn"
  tar -xf "$t" -C "$bn"
done
}

package_distro-grub-themes() {
  pkgdesc="Metapackage for installing grub2 distro themes"  
  depends=("grub-theme-acer"
        "grub-theme-alma"
        "grub-theme-aorus"
        "grub-theme-apple"
        "grub-theme-arch"
        "grub-theme-arco"
        "grub-theme-artix"
        "grub-theme-asrock"
        "grub-theme-asus"
        "grub-theme-bedrock"
        "grub-theme-centos"
        "grub-theme-chromeos"
        "grub-theme-debian"
        "grub-theme-deepin"
        "grub-theme-dell"
        "grub-theme-elementary"
        "grub-theme-endeavouros"
        "grub-theme-fedora"
        "grub-theme-framework"
        "grub-theme-freebsd"
        "grub-theme-garuda"
        "grub-theme-gentoo"
        "grub-theme-gigabyte"
        "grub-theme-hp"
        "grub-theme-huawei"
        "grub-theme-kdeneon"
        "grub-theme-kingston"
        "grub-theme-kubuntu"
        "grub-theme-legion"
        "grub-theme-lenovo"
        "grub-theme-lg"
        "grub-theme-lubuntu"
        "grub-theme-manjaro"
