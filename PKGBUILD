pkgname='jaguar-bin'
pkgver=1.9.25
pkgrel=1
pkgdesc="Tool to develop Toit programs for the ESP32"
arch=('x86_64')
url="https://github.com/toitlang/jaguar"
license=('MIT')
depends=('gcc-libs')
provides=('jaguar')
conflicts=('jaguar')
source=("$pkgname-$pkgver.tgz::$url/releases/download/v$pkgver/jag_linux.tgz")
sha256sums=('099d770f0ecef55a46eae88ce1e3768c62709af5b1b8e232f936a54cae756550')

package() {
	install -D jag -t "$pkgdir/usr/bin/"
}
