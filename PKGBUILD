# -*- sh -*-

pkgname='python-uv-latest-bin'
pkgver=0.0.0
pkgrel=0
epoch=1
arch=('any')
pkgdesc='An extremely fast Python package installer and resolver (please use extra/uv instead)'
url='https://github.com/astral-sh/uv'
license=('Apache-2.0 OR MIT')  # SPDX-License-Identifier: Apache-2.0 OR MIT
source=('README.md')
sha256sums=('SKIP')

_scheduled_for_deletion() {
  cat <<EOT >&2
This package has been scheduled for deletion.
Please use extra/uv and extra/python-uv instead.
EOT
  false
}

build() {
  _scheduled_for_deletion
}

package() {
  _scheduled_for_deletion
}

# eof
