pkgname=clean
pkgver=1.0
pkgrel=1
pkgdesc="System cleaner"
arch=('any')
license=('GPL')
depends=('bash' 'bc')  # Specifica le dipendenze del pacchetto, se ce ne sono

package() {
   install -Dm755 clean "$pkgdir/usr/bin/clean"
}
